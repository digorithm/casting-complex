{
  "name": "express-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "seed": "./server/node_modules/.bin/sequelize --config ./server/config/config.js --seeders-path ./server/seeders db:seed:all",
    "start-server": "DEBUG=express-sequelize node ./server/bin/www",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --timeout 5000 test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --timeout 5000 test/integration/*.test.js"
  },
  "dependencies": {
    "aglio": "^2.3.0",
    "bcrypt": "^1.0.3",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "chai-http": "^4.0.0",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "expect": "^22.4.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "morgan": "^1.7.0",
    "pg": "^6.1.0",
    "pg-native": "^2.2.0",
    "pug": "^2.0.0-rc.4",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^3.1.0",
    "serve-favicon": "~2.3.0",
    "sqlite3": "^3.0.8",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^4.1.2",
    "cross-env": "^3.1.3",
    "eslint": "^3.19.0",
    "expect.js": "^0.3.1",
    "mocha": "^3.5.3",
    "supertest": "^2.0.0"
  }
